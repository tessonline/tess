								<xs:annotation>
									<xs:documentation>
                    obchodní balík
                  </xs:documentation>
								</xs:annotation>
							</xs:enumeration>
						</xs:restriction>
						<!--CHANGE 2011-04-28 Změně výčtu povolených typů - aktualizace do stavu podle České pošty -->
